@model IQTech.Models.Fact

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>IQTech Post</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label style="color: white;" asp-for="IqQuestion" class="control-label">IQ Question</label>
                <input asp-for="IqQuestion" class="form-control" />
                <span asp-validation-for="IqQuestion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label style="color: white;" asp-for="IqAnswer" class="control-label">IQ Answer</label>
                <input asp-for="IqAnswer" class="form-control" />
                <span asp-validation-for="IqAnswer" class="text-danger"></span>
            </div>
            </br></br>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
</br></br>
<div>
    <button style="padding: 15px 32px; cursor: pointer; background-color: #ffffff; border-radius: 12px;"><a style="color: darkblue;" asp-action="Index">Back to List</a></button>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
